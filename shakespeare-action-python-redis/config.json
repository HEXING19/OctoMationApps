    "configuration": {
        "host": {
            "data_type": "string",
            "description": "Redis服务器主机名或者IP地址",
            "required": true,
            "options": "",
            "order": 0
        },
        "port": {
            "data_type": "integer",
            "description": "Redis对外开放的端口",
            "default_value": "6379",
            "required": false,
            "options": "",
            "order": 1
        },
        "db": {
            "data_type": "integer",
            "description": "Redis db（默认为：0）",
            "default_value": "0",
            "required": false,
            "options": "",
            "order": 2
        },
        "password": {
            "data_type": "password",
            "description": "Redis认证密码",
            "required": false,
            "options": "",
            "order": 3
        },
        "socket_timeout_seconds": {
            "data_type": "integer",
            "description": "Socket超时数（秒），默认永不过期",
            "default_value": "",
            "options": "",
            "required": false,
            "order": 4
        },
        "encode": {
            "data_type": "string",
            "description": "字符串编码，默认：utf-8",
            "default_value": "utf-8",
            "options": "",
            "required": false,
            "order": 5
        }
    },
